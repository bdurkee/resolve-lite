Facility U;
        uses Standard_Integers, Standard_Booleans;

    Operation Do_Nothing (restores i : Std_Integer_Fac :: Integer);
        Procedure
            i++;
            i--;
    end Do_Nothing;

    Operation Main ();
        Procedure
            Var x : Std_Integer_Fac :: Integer;
            Var a, y, z : Std_Boolean_Fac :: Boolean;
            Do_Nothing(x);
    end Main;
end U;