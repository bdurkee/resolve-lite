Concept Locn_Referencing_Template<T>(evaluates k : Std_Integer_Fac :: Integer);
        uses Standard_Integers, Basic_Integer_Theory, Function_Theory,
            Closure_Op_Ext, Basic_Natural_Number_Theory;
        requires 1 <= k which_entails k : N;

    Defines Location : SSet;
    Defines Void : Location;
    Defines Ocpn_Disp_Incr, Total_Mem_Cap : N;
        Constraints ||Location|| > Total_Mem_Cap / Ocpn_Disp_Incr;

    //Var
end Locn_Referencing_Template;