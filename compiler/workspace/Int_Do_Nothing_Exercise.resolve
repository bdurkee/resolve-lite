Facility Int_Do_Nothing_Exercise;
        uses Standard_Integers;
    Operation Do_Nothing (restores i : Std_Integer_Fac :: Integer);
        Procedure
            i++;
            i--;
    end Do_Nothing;

    Operation Main ();
        Procedure
            Var x : Std_Integer_Fac :: Integer;
            Do_Nothing(x);
    end Main;
end Int_Do_Nothing_Exercise;