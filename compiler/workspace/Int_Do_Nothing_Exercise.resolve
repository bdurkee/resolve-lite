Facility Int_Do_Nothing_Exercise;
        uses Standard_Integers, Basic_Integer_Theory;
    Operation Do_Nothing (restores i : Std_Integer_Fac :: Integer);
           requires i = 0;
        Procedure //i++; i--;
    end Do_Nothing;

    Operation Main ();
        Procedure
            Var t : Std_Integer_Fac :: Integer;
            Do_Nothing(t);
    end Main;
end Int_Do_Nothing_Exercise;