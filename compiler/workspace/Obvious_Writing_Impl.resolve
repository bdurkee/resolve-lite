Implementation Obvious_Writing_Impl(Operation Write_Entry(evaluates e : E);)
		for Writing_Capability of Bdd_Stack_Template;
			uses Standard_Integers;
	Procedure Write(clears S : Stack);
		Var Next : E;
		While (1 <= Depth(S)) do
			Pop(Next, S);
			Write_Entry(Next);
		end;
	end Write;
end Obvious_Writing_Impl;