Concept Integer_Template;
        uses Basic_Integer_Theory;

    Type family Integer is modeled by Z;
        exemplar I;
        initialization
                ensures I = 0;

    Operation Sum
            (updates a, b : Integer)
                : Integer;
        ensures Sum = (a + b);

    Operation Difference
            (updates a, b : Integer)
                : Integer;
        ensures true;


end Integer_Template;