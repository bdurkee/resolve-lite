Facility Reverse_Stack;
    Facility SF is Bdd_Stack_Template
        <Std_Integer_Fac::Integer>(1) implemented by Array_Impl;

    Operation Read_Stack(replaces S: SF::Stack);
        Procedure
            Var CS, RC: Std_Integer_Fac::Integer;
            SF::Clear(S);

            RC := SF::Rem_Capacity(S);
            SF::Push(CS, S);


    end Read_Stack;

end Reverse_Stack;