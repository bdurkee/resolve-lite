Concept Boolean_Template;

    Type Family Boolean is modeled by B;
        exemplar b;
        initialization ensures b = true;

    Oper True (): Boolean;
        ensures True = true;

    Oper False (): Boolean;
        ensures False = false;

    Oper And (evaluates a, b: Boolean): Boolean;
        ensures And = (a and b);

    Oper Or (evaluates a, b: Boolean): Boolean;
        ensures Or = (a or b);

end Boolean_Template;